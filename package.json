{
  "name": "pup-api",
  "version": "1.0.0",
  "author": "",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "watched": "cross-env NODE_ENV=development node -r dotenv/config -r ts-node/register ./src/index.ts",
    "dev": "nodemon",
    "build": "tsc && npm run start",
    "start": "cross-env NODE_ENV=production node dist/index.js"
  },
  "engines": {
    "node": "12.16.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "./src/"
    ],
    "exec": "npm run watched",
    "ext": "ts"
  },
  "license": "ISC",
  "dependencies": {
    "@types/puppeteer": "^5.4.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "puppeteer": "^5.5.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.10",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  }
}